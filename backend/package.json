  "description": "A simple Koa project for learning purposes",
  "main": "main.js",
  "scripts": {
    "start:api": "node app.js",
    "start:resource": "sleep 2 && node APIGateway/AuthServices/Resource/resource-services.js", 
    "start:stream": "sleep 2 && node APIGateway/AuthServices/Stream/stream-services.js",
    "start:log": "sleep 2 && node APIGateway/LogServices/log-services.js",  
