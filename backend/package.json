    "jpeg-js": "^0.4.4",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.15.3",
    "koa-bodyparser": "^4.4.1",
    "koa-jwt": "^4.0.4",
    "koa-router": "^12.0.1",
    "koa-send": "^5.0.1",
    "koa-static": "^5.0.0",
    "koa-stream": "^0.2.0",
